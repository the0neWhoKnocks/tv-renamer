{
  "name": "@noxx/tv-renamer",
  "version": "2.11.0",
  "description": "A local network app that allows for renaming TV shows",
  "main": "index.js",
  "paths": {
    "babelOutput": "./dist/cjs",
    "babelSource": "./src"
  },
  "scripts": {
    "build:app": "cross-env MODE=production webpack",
    "build:dist": "node ./bin/prepDist && npm run transpile && node ./bin/pruneDist",
    "build:appForDocker": "cross-env NODE_ENV=production npm run build:dist && cross-env NODE_ENV=production npm run build:app",
    "e2e": "./e2e/bin/test-runner.sh",
    "e2e:watch": "npm run e2e -- --watch",
    "gen:files": "node ./e2e/bin/genFiles",
    "lint": "eslint ./src/**/*.js",
    "release": "./bin/release.js",
    "release:dryrun": "./bin/release.js -dr",
    "release:update": "sh -c \"$(curl -fsSL https://raw.githubusercontent.com/the0neWhoKnocks/release-script/master/js/tools/install.sh) --update --install-dir \\\"bin\\\"\"",
    "start": "npm run build:appForDocker && cross-env NODE_ENV=production npm run start:server",
    "start:dev": "npm run build:dist && cross-env DEBUG=true PORT=9000 concurrently --kill-others -p {name} -n BABEL,WEBPACK,SERVER -c black.bgGreen,black.bgCyan,black.bgMagenta \"npm run watch:transpile\" \"npm run watch:app\" \"npm run watch:server\"",
    "start:server": "node $npm_package_paths_babelOutput/server",
    "test": "npm run lint && npm run e2e",
    "transpile": "babel $npm_package_paths_babelSource -d $npm_package_paths_babelOutput --verbose",
    "watch:app": "cross-env MODE=development webpack --watch",
    "watch:server": "babel-node ./src/server/watcher",
    "watch:transpile": "npm run transpile -- --skip-initial-build --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/the0neWhoKnocks/tv-renamer.git"
  },
  "author": "",
  "dependencies": {
    "debug": "^4.2.0",
    "mkdirp": "^0.5.1",
    "rimraf": "^2.6.3",
    "sanitize-filename": "^1.6.1",
    "teeny-request": "^7.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.3",
    "@babel/node": "^7.2.2",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@noxx/babel-plugin-a2rp": "^1.0.1",
    "@noxx/eslint-config": "^1.0.7",
    "babel-loader": "^8.0.5",
    "babel-plugin-emotion": "^10.0.7",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-transform-define": "^1.3.1",
    "browser-sync": "^2.26.12",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^4.1.2",
    "cross-env": "^5.2.0",
    "emotion": "^10.0.7",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "eslint": "^6.7.0",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-require-jsdoc-except": "^1.3.0",
    "fs-extra": "^7.0.1",
    "nodemon": "^1.18.10",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "simple-progress-webpack-plugin": "^1.1.2",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-manifest-plugin": "^2.2.0"
  }
}
